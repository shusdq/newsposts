{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "extends": ["airbnb" ],
    "rules": {
        "indent": "off",
        "linebreak-style": 0,
        "no-console": "off",
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "import/extensions": "off"
    },
    "settings": {
        "react": {
          "version": "detect"
        }
  }
}